
@{
    ViewBag.Title = "List";
}

<h2>List</h2>
<a href="/department/create">Create</a>
<table class="table table-bordered" >
    <tr>
        <th>Id</th>
        <th>Name</th>
    </tr>
    @foreach (var item in Model) { 
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
        </tr>
    }
</table>

